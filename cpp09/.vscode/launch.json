{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch ex00",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ex00/btc",
            "args": ["input.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/ex00",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build ex00",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}